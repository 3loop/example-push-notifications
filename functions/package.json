{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "fix": "eslint --ext .js,.ts . --fix",
    "format": "prettier --write .",
    "build": "rollup -c rollup.config.mjs",
    "build:watch": "tsc --watch",
    "serve": "pnpm run build && firebase emulators:start --only functions",
